class Counter {
    var count = 5;
}

def $main() {
    var num = 5;
    echo num;

    num += 1;
    echo num;

    num -= 2;
    echo num;

    echo;
    var counter = Counter();
    echo counter.count;

    counter.count += 1;
    echo counter.count;

    counter.count -= 2;
    echo counter.count;
}
